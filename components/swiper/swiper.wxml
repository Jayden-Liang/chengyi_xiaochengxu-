<!--components/swiper/swiper.wxml-->
<swiper 
  indicator-dots='true'
  autoplay='true'
  circular='true'
  interval='3000'
  indicator-color="#ffffff"
  indicator-active-color="#e6e6fa"
  >
   <block wx:for="{{imaglocation}}" wx:key="index">
    <swiper-item>
      <image class="swiper-image" src="{{item}}" mode='widthFix'/>
    </swiper-item>
  </block>
</swiper>
